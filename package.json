{
  "name": "audio-video-merger",
  "productName": "audio-video-merger",
  "version": "1.1.0",
  "description": "A tool for painlessly replacing audio under video files",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "TEST=true wdio run wdio.conf.ts"
  },
  "keywords": [],
  "author": {
    "name": "kapoko",
    "email": "kasper.koman@gmail.com"
  },
  "license": "GPL-3.0",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": ["--loader=ts-node/esm"],
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard-scss",
    "customSyntax": "postcss-scss",
    "rules": {
      "max-nesting-depth": 2,
      "indentation": 4
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      },
      "useJSXTextNode": true,
      "project": "./tsconfig.json"
    },
    "rules": {
      "indent": ["error", 4]
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/recommended",
      "plugin:import/electron",
      "plugin:import/typescript"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/ffprobe-static": "^2.0.0",
    "@types/fluent-ffmpeg": "^2.1.19",
    "@types/lodash": "^4.14.176",
    "@types/mime-types": "^2.1.1",
    "@types/mmmagic": "^0.4.30",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@wdio/cli": "^8.39.1",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/spec-reporter": "^8.39.0",
    "electron": "^31.3.1",
    "eslint": "^8.57.0",
    "mime-types": "^2.1.34",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.8",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-webpack-plugin": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "wdio-electron-service": "^6.6.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@mui/icons-material": "^5.16.6",
    "@mui/material": "^5.16.6",
    "electron-squirrel-startup": "^1.0.0",
    "ffmpeg-static": "^4.4.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transition-group": "^4.4.2"
  }
}
